# Gaia GSRPar Makefile

COMPILER = /usr/bin/g++
COMPILERCPP = /usr/bin/g++

CC = $(COMPILER)
CPP= $(COMPILERCPP)

ROOT=/Users/ube/VisIVOServer_OACT/2.3/
ROOTFIL=$(ROOT)/VisIVOFilters/Filters/

VSFILINC1=/Users/ube/VisIVOServer_OACT/2.3/VisIVOFilters/Filters
VSFILINC2=/Users/ube/VisIVOServer_OACT/2.3/Utils

VSFIL= $(ROOT)/Utils/fparser.o	$(ROOTFIL)/vsLine.o			$(ROOTFIL)/vscoarsevolumeop.o		$(ROOTFIL)/vsmathop.o			$(ROOTFIL)/vsprinttableop.o	$(ROOTFIL)/vsstatisticop.o 	$(ROOTFIL)/fpoptimizer.o		$(ROOTFIL)/vsVector.o			$(ROOTFIL)/vscutop.o			$(ROOTFIL)/vsmergeop.o			$(ROOTFIL)/vsrandomizertableop.o		$(ROOTFIL)/vsswapop.o $(ROOTFIL)/mainFilter.o			$(ROOTFIL)/vsVoxel.o			$(ROOTFIL)/vsdecimatorop.o		$(ROOTFIL)/vsmoduleop.o	$(ROOTFIL)/vsselectcolumnsop.o		$(ROOTFIL)/vstable.o $(ROOT)/Utils/parametersparser.o		$(ROOTFIL)/vsaddidop.o		$(ROOTFIL)/vsexampleop.o		$(ROOTFIL)/vsmrcampos.o	$(ROOTFIL)/vsselectfieldop.o		$(ROOTFIL)/vstableop.o $(ROOT)/Utils/tinystr.o		$(ROOTFIL)/vsextractlistrowsop.o		$(ROOTFIL)/vsmuportalop.o		$(ROOTFIL)/vsselectvol.o			 $(ROOT)/Utils/tinyxml.o				$(ROOTFIL)/vsextractsubvolumeop.o	$(ROOT)/Utils/vsobject.o			$(ROOTFIL)/vsshowtableop.o		$(ROOTFIL)/vsvisualop.o $(ROOT)/Utils/tinyxmlerror.o		$(ROOTFIL)/vsappend.o			$(ROOTFIL)/vsgrid2pointdistr.o		$(ROOTFIL)/vspointdistributeop.o		$(ROOTFIL)/vssigmacontoursop.o		$(ROOTFIL)/vsvollimit.o $(ROOT)/Utils/tinyxmlparser.o		$(ROOTFIL)/vschangecolnameop.o		$(ROOTFIL)/vsincludeop.o			$(ROOTFIL)/vspointpropertyop.o		$(ROOTFIL)/vssphereop.o			$(ROOTFIL)/vswrvotableop.o 	$(ROOT)/Utils/util.o			$(ROOTFIL)/vsclustercellop.o		$(ROOTFIL)/vsinterpolateop.o		$(ROOTFIL)/vspolarop.o		$(ROOTFIL)/vssplittableop.o $(ROOT)/Utils/historyXmlWriter.o $(ROOTFIL)/startFilter.o

INCLUDE = -I$(VSFILINC1) -I$(VSFILINC2)
#INCLUDE = -I$(GAIAINC) 
#CFLAGS= $(INCLUDE)  -std=c99 
#CPPFLAGS= $(INCLUDE) -g -fsanitize=address -mllvm -asan-stack
CPPFLAGS= $(INCLUDE) 
LIB =  -lm


all: VisIVOFilter 

VisIVOFilter: $(VSFIL)
	$(CPP) $(CPPFLAGS) -o VisIVOFilter $(VSFIL) $(INCLUDE) $(LIB)


	
clean:
	rm -f *.o ../../Utils/*.o core
